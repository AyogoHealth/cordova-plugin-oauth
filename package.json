  "repository": "github:AyogoHealth/cordova-plugin-oauth.git",
  "keywords": [
    "cordova",
    "ecosystem:cordova",
    "cordova-android",
    "cordova-ios"
  ],
  "files": [
    "src",
    "www",
    "plugin.xml"
  ],
  "scripts": {
    "version": "sed -i -e \"s/\\(<plugin [^>]*id=\\\"$npm_package_name\\\" [^>]*version=\\\"\\)[^\\\"]*\\\"/\\1$npm_package_version\\\"/\" plugin.xml && git add plugin.xml"
