    "platforms": [
      "ios",
      "android"
    ]
  },
  "engines": {
    "cordovaDependencies": {
      "1.0.0": {
        "cordova-android": ">= 8.0.0",
        "cordova-ios": ">= 5.0.0"
      },
      "2.0.0": {
        "cordova-android": ">= 8.0.0",
        "cordova-ios": ">= 5.0.0"
      },
      "3.0.0": {
        "cordova-android": ">= 8.0.0",
        "cordova-ios": ">= 6.1.0"
      },
      "4.0.0": {
        "cordova-android": ">= 9.0.0",
        "cordova-ios": ">= 6.1.0"
      },
      "5.0.0": {
        "cordova": ">100"
      }
    }
  },
  "dependencies": {},
  "devEngines": {
    "packageManager": {
      "name": "npm",
      "onFail": "ignore"
    }
  }
